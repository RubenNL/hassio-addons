{
  "name": "Pilight",
  "version": "1.1.14",
  "slug": "pilight",
  "description": "Pilight Service Addon",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "host_network": "true",
  "privileged": ["SYS_RAWIO"],
  "gpio": "true",
  "devices": ["/dev/mem:/dev/mem:rwm"],
  "options": {
    "gpio_layout": "raspberrypi3"
  },
  "schema": {
    "gpio_layout": "str"
  },
  "ports": {
    "5000/tcp": 5010
  }
}
